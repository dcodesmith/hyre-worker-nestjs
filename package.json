  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@commitlint/cli": "^20.4.0",
    "@commitlint/config-conventional": "^20.4.0",
    "@flydotio/dockerfile": "^0.7.10",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^11.1.8",
    "@swc/core": "^1.13.5",
    "@testcontainers/postgresql": "^11.8.0",
    "@testcontainers/redis": "^11.8.0",
    "@types/express": "^5.0.6",
    "@types/node": "^20.19.13",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "dotenv-cli": "^11.0.0",
    "husky": "^9.1.7",
    "pino-pretty": "^13.1.3",
    "prisma": "^6.19.0",
    "supertest": "^7.1.4",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2",
    "unplugin-swc": "^1.5.7",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=22.12.0"
  },
  "pnpm": {
    "overrides": {
      "body-parser": "2.2.1",
      "lodash": "4.17.23",
      "minimatch": "10.2.2",
      "qs": "6.14.2",
      "rollup": "4.53.5"
    }
  },
