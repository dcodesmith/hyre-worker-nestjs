{
  "name": "hyre-worker",
  "version": "1.0.0",
  "packageManager": "pnpm@10.13.1",
  "scripts": {
    "build": "npx prisma generate && nest build",
    "format": "biome format --write src",
    "lint": "biome lint src",
    "lint:fix": "biome lint --write src",
    "check": "biome check src",
    "check:fix": "biome check --write src",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:coverage:ci": "vitest --coverage --run && npx nyc report --reporter=lcov --temp-dir=coverage --report-dir=coverage",
    "test:ui": "vitest --ui",
    "db:generate": "prisma generate",
    "deploy": "pnpm run build && pnpm run db:generate"
  },
  "dependencies": {
    "@bull-board/api": "^6.14.0",
    "@bull-board/express": "^6.14.0",
    "@bull-board/nestjs": "^6.14.0",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/common": "^11.1.8",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.8",
    "@nestjs/platform-express": "^11.1.8",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^6.19.0",
    "@react-email/components": "^0.3.3",
    "@react-email/render": "^1.2.1",
    "axios": "^1.11.0",
    "bullmq": "^5.63.0",
    "date-fns": "^4.1.0",
    "ioredis": "^5.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reflect-metadata": "^0.1.14",
    "resend": "^2.1.0",
    "rxjs": "^7.8.2",
    "twilio": "^4.23.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@flydotio/dockerfile": "^0.7.10",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^11.1.8",
    "@swc/core": "^1.13.5",
    "@types/node": "^20.19.13",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "prisma": "^6.19.0",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2",
    "unplugin-swc": "^1.5.7",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
