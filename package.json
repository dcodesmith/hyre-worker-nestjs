{
  "name": "hyre-worker",
  "version": "1.0.0",
  "packageManager": "pnpm@10.20.0",
  "scripts": {
    "build": "npx prisma generate && nest build",
    "format": "biome format --write src",
    "lint": "biome lint src",
    "lint:fix": "biome lint --write src",
    "check": "biome check src",
    "check:fix": "biome check --write src",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:coverage:ci": "vitest --coverage --run && npx nyc report --reporter=lcov --temp-dir=coverage --report-dir=coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "npx dotenv -e .env.e2e -o -- vitest run --config vitest.e2e.config.mjs",
    "db:generate": "prisma generate",
    "deploy": "pnpm run build && pnpm run db:generate",
    "prepare": "husky",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.994.0",
    "@bull-board/api": "^6.14.0",
    "@bull-board/express": "^6.14.0",
